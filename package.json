{
    "name": "agentic-wiki",
    "displayName": "Agentic Wiki",
    "description": "The Agentic Wiki For Your Git Codebase",
    "version": "0.0.4",
    "publisher": "fine405",
    "author": {
        "name": "fine"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/fine405/vsc-extension-agentic-wiki"
    },
    "homepage": "https://github.com/fine405/vsc-extension-agentic-wiki",
    "bugs": {
        "url": "https://github.com/fine405/vsc-extension-agentic-wiki/issues"
    },
    "keywords": [
        "wiki",
        "documentation",
        "codebase",
        "agent",
        "pocketflow",
        "deepwiki"
    ],
    "engines": {
        "vscode": "^1.99.0"
    },
    "categories": [
        "AI"
    ],
    "activationEvents": [],
    "icon": "icon.png",
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "agentic-wiki.config",
                "title": "Agentic Wiki: Config"
            },
            {
                "command": "agentic-wiki.generate",
                "title": "Agentic Wiki: Generate"
            },
            {
                "command": "agentic-wiki.open",
                "title": "Agentic Wiki: Open"
            }
        ]
    },
    "scripts": {
        "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
        "watch": "npm-run-all -p watch:*",
        "watch:esbuild": "node esbuild.js --watch",
        "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
        "build": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
        "package": "pnpm vsce package --no-dependencies",
        "package:pre": "pnpm vsce package --pre-release --no-dependencies",
        "publish:pre": "pnpm vsce publish --pre-release --no-dependencies",
        "publish": "pnpm vsce publish --no-dependencies",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
        "check-types": "tsc --noEmit",
        "lint": "eslint src",
        "lint:fix": "eslint --fix src",
        "test": "vscode-test",
        "format": "prettier --write . src"
    },
    "devDependencies": {
        "@types/debug": "^4.1.12",
        "@types/mocha": "^10.0.10",
        "@types/node": "20.x",
        "@types/vscode": "^1.99.0",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1",
        "esbuild": "^0.25.1",
        "eslint": "^9.23.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.5.3",
        "pretty-quick": "^4.1.1",
        "simple-git-hooks": "^2.13.0",
        "typescript": "^5.8.2"
    },
    "simple-git-hooks": {
        "pre-commit": "pretty-quick --staged"
    },
    "dependencies": {
        "debug": "^4.4.0",
        "ignore": "^7.0.4",
        "minimatch": "^10.0.1",
        "openai": "^4.96.2",
        "pocketflow": "^1.0.4",
        "yaml": "^2.7.1"
    }
}
